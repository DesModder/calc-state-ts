        "center",
        "center_auto",
        "auto_center",
        "above",
        "above_left",
        "above_right",
        "above_auto",
        "below",
        "below_left",
        "below_right",
        "below_auto",
        "left",
        "auto_left",
        "right",
        "auto_right"
